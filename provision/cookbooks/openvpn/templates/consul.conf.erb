[program:consul_<%= node['attributes'].fetch('cluster_id') %>]
command=/usr/bin/consul agent -ui -config-file /etc/consul/consul_<%= node['attributes'].fetch('cluster_id') %>.json
autostart=true
autorestart=true
stderr_logfile=/data/consul_logs/consul_<%= node['attributes'].fetch('cluster_id') %>.log
stdout_logfile=/data/consul_logs/consul_<%= node['attributes'].fetch('cluster_id') %>.log